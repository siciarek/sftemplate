{% extends "::base.html.twig" %}

{% block body %}

    {{ page.content|raw }}

    <script>
        jQuery(document).ready(function ($) {

            $('a:contains("__URL__")')
                .attr('href', '{{ app.request.schemeAndHttpHost }}')
                .html('{{ app.request.schemeAndHttpHost }}')
            ;
            $('p:contains("__PROVIDER__")')
                .html('<b>{{ app_name|trans }}</b>')
            ;
        });
    </script>

{% endblock %}
